#include <stdio.h>
#include <stdlib.h>
#include <time.h>


int main (int argc, char *argv[]){
    srand(time(NULL));
    int vector[100];
    int i;
    int contador;
    int tamaño = sizeof(vector) / sizeof(vector[0]);

    for (i = 0; i < tamaño; i++){
        vector [i] = rand () % 100 + 1;
    }

    for (int j = 0; j < 100; j++)
    {
        for (int k = 0; k < 100; k++)
        {
            if (vector[k] > vector[k+1]) {
                contador = vector[k+1];
                vector[k+1] = vector [k];
                vector[k] = contador;

            }
        }
    }
    for (int l = 0; l < 100; l++)
    {
        printf("%d\t", vector [l]);
    }
    
    return 0;
}
